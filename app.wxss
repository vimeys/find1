/**app.wxss**/
@import "pages/index.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
page{
  background: #fafafa;
}

view,image,text,navigator{
  box-sizing: border-box;
  padding:0;
  margin:0;
}

.btn{
  width: 690rpx;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  line-height: 88rpx;
  height: 88rpx;
  margin: 0 auto;
  background: #3C6CB8;
  border-radius: 10rpx;
}
